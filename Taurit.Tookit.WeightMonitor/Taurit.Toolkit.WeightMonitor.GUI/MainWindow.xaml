<Window x:Class="Taurit.Toolkit.WeightMonitor.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Taurit.Toolkit.WeightMonitor.GUI"
        xmlns:wpf="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel LastChildFill="True">
       
        <Button DockPanel.Dock="Bottom" Click="Button_Click" >Save To file</Button>
        <wpf:CartesianChart Name="xchart" Grid.Row="2" Margin="10, 0, 10, 20"  Hoverable="False" DataTooltip="{x:Null}" Background="White">
            <wpf:CartesianChart.Series>
                <wpf:ColumnSeries Values="5,9,8,6,1,5,7,3,6,3"/>
            </wpf:CartesianChart.Series>
        </wpf:CartesianChart>
    </DockPanel>
</Window>
